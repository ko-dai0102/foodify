<div id="like-btn<%= item.id %>">
  <% if user_signed_in? && item.liked_by?(current_user)%>
    <%=link_to "いいね済", item_likes_path(item.id),  data: { turbo_method: :delete }, class: "tweet_likes" %>
  <% else %>
    <%=link_to "いいねする", item_likes_path(item.id), data: { turbo_method: :post }, class: "tweet_likes" %>
  <% end %>
</div>